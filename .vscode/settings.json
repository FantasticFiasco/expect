// Place your settings in this file to overwrite default and user settings.
{
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/*.js": {"when": "$(basename).ts"},
    "**/*.d.ts": {"when": "$(basename).ts"},
    "**/*.js.map": true,
    "**/npm-debug.log": true
  },
  "tslint.exclude": "**/test/**/*.ts",
  "typescript.tsdk": "node_modules/typescript/lib"
}
